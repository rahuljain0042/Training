class Grandparent{
int a;

Grandparent(int a)
{
this.a=a;
}
}
class parent extends Grandparent{
int b;

Parent(int a, int b){
super(a);
this.b=b;
}

void show() {
System.out.println("Grandparent's a ="+a);
System.out.println("Parent's b  =" +b);
}
}
class Child{
public static void main(String args[]){
Parent object = new parent(8,9);
object.show();
}
}